{
	"server": {
		"http_addr": ":4443",
		"tls_key_file": "./notary-server.key",
		"tls_cert_file": "./notary-server.crt"
	},
	"trust_service": {
		"type": "remote",
		"hostname": "notarysigner",
		"port": "7899",
		"tls_ca_file": "./root-ca.crt",
		"key_algorithm": "ecdsa",
		"tls_client_cert": "./notary-server.crt",
		"tls_client_key": "./notary-server.key"
	},
	"logging": {
		"level": "debug"
	},
	"storage": {
		"backend": "mysql",
		"db_url": "server@tcp(mysql:3306)/notaryserver?parseTime=True"
	},
    "auth": {
        "type": "token",
        "options": {
            "realm": "https://10.117.4.142/service/token",
            "service": "notary-server",
            "issuer": "registry-token-issuer",
            "rootcertbundle": "/go/src/github.com/docker/notary/fixtures/token_root.crt"
        }
    }
}
